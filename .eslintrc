{
	"extends": [
		"eslint:recommended"
	],
	"rules": {
		"no-console": 0,
		"quotes": [
			"warn",
			"single"
		],
		"semi": [
			"warn",
			"always"
		]
	},
	"globals": {
		"window": true,
		"document": true,
		"localStorage": true,
		"FormData": true,
		"FileReader": true,
		"Blob": true,
		"navigator": true
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 8,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true,
			"classes": true,
			"impliedStrict": true
		}
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	}
}
